.mode box
.headers on
-- SELECT customers.phone FROM customers;

-- SELECT orders.shipped FROM orders;

SELECT 	customers.name, orders.shipped, customers.phone
FROM 	customers
JOIN 	orders ON orders.customerid = customers.customerid
ORDER BY orders.shipped ASC
LIMIT 	10;
